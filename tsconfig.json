{
  "compilerOptions": {
      "target": "ES2019",
      "sourceMap": true,
      "module": "commonjs",
      "esModuleInterop": true,
      "declaration": true,
      "resolveJsonModule": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noImplicitReturns": true,
      "strictNullChecks": true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "skipLibCheck": true,
      "outDir": "dist",
      "lib": [
          "dom",
          "ES2019"
      ],
      "moduleResolution": "node",
      "types": [
          "node"
      ],
      "incremental": true,
      "composite": true,
      "baseUrl": "./"
  }
}
